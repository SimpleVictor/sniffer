{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "inlineSourceMap": true,
        "inlineSources": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "declaration": true,
        "lib": [
            "dom",
            "ES2015"
        ]
    },
    "exclude": [
        "../node_modules",
        "node_modules"
    ]
}
